<view class="am-list {{className}}">
  <view class="am-list-header" a:if="{{$slots.header}}">
    <slot name="header" />
  </view>
  <view class="am-list-body"  a:if="{{$slots.items}}">
    <slot name="items" />
  </view>
  <view class="am-list-footer" a:if="{{$slots.footer}}">
    <slot name="footer" />
  </view>
</view>
