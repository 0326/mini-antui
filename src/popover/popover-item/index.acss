.popover-item {
  min-width: 80px;
  max-width: 170px;
  height: 39px;
  margin: 0 10px;
  line-height: 39px;
  font-size: 14px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #333333;
  text-align: center;
}
.popover-item:not(:last-child) {
  border-bottom: 1px solid #eeeeee;
}
